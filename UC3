
import java.util.Scanner;

public class UC3{

public static final Scanner sc = new Scanner(System.in);

    public static void main(String args[]){
        System.out.println(String.join(" ", "   ** ", "       ** ", "      ****** ","         ***** "));
        System.out.println(String.join(" ", " **   ** ", "  **   ** ", "   **    ** ","     ** "));
        System.out.println(String.join(" ", "**     **", " **     ** ", "  **      **","   ** "));
        System.out.println(String.join(" ", "**     **", " **      ** ", " **      ** ","   **"));
        System.out.println(String.join(" ", "**     **", " **      ** ", " **    ** ", "      *** "));
        System.out.println(String.join(" ", "**     **", " **      ** ", " ******","            **"));
        System.out.println(String.join(" ", "**     **", " **      ** ", " ** ","                **"));
        System.out.println(String.join(" ", "  ** ** ", "   **    ** ", "  ** ","               **"));
        System.out.println(String.join(" ", "   ** ", "        ** ", "     ** ","          ***** "));

    }


}
